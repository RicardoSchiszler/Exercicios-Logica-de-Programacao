algoritmo "semnome"
// Função :
// Autor :
// Data : 22/06/2021
// Seção de Declarações 

procedimento somaDois(n1, n2: real)
Inicio

   Escreval("Recebi: ", n1)
   Escreval("Recebi: ", n2)
   

   Escreval("Retornei: ", n1 + n2)


FimProcedimento

var
  n1,n2 : real
inicio
// Seção de Comandos 

   somaDois(n1,n2)


fimalgoritmo
