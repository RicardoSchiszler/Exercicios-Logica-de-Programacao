algoritmo "Vetorzinho"
// Função :
// Autor :
// Data : 22/06/2021
// Seção de Declarações 
var
     n : vetor[1..10] de inteiro
     i : inteiro
inicio
// Seção de Comandos 

   Para i <- 1 ate 10 faca
          Escreva("Digite o ", i, " .o numero: ")
          Leia(n[i])
   FimPara
   
   Para i <- 1 ate 10 faca
          Escreva(n[i])
   FimPara

fimalgoritmo
